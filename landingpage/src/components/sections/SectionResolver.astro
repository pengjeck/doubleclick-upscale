---
import type { CollectionEntry } from 'astro:content'
import Features from './Features.astro'

interface Props {
  afterTitle: string,
  sect: CollectionEntry<'sections'>,
  title: string,
  type: string
}

const { afterTitle, sect, title, type } = Astro.props
const { Content } = await sect.render()
---

{type === 'features' && <Features afterTitle={afterTitle} title={title}>
  <Content />
</Features>}

